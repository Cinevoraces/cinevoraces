{
    "name": "cinevoraces-api",
    "version": "2.0.0",
    "scripts": {
        "dev": "node ./scripts/dev.mjs",
        "dev:local": "node ./scripts/dev.mjs --exportEnv=.env",
        "build": "tsc && tsc-alias",
        "start": "node ./dist/src/index.js",
        "test": "jest --watchAll",
        "test:unit": "node ./scripts/test.mjs",
        "test:unit:local": "node ./scripts/test.mjs --exportEnv=.env",
        "test:int": "jest --watchAll ./tests/integration/*",
        "lint": "eslint --fix **/*.ts",
        "format": "prettier --write \"src/**/*.ts\""
    },
    "dependencies": {
        "@fastify/cookie": "^9.3.1",
        "@fastify/cors": "^9.0.1",
        "@fastify/jwt": "^8.0.0",
        "@fastify/multipart": "^8.1.0",
        "@fastify/rate-limit": "^9.1.0",
        "@fastify/static": "^7.0.0",
        "@fastify/swagger": "^8.14.0",
        "@fastify/swagger-ui": "^2.1.0",
        "@getbrevo/brevo": "^2.0.0-beta.4",
        "bcryptjs": "^2.4.3",
        "cloudinary": "^2.0.0",
        "dotenv": "^16.4.4",
        "fastify": "^4.26.0",
        "fastify-plugin": "^4.5.1",
        "pg": "^8.11.3",
        "qs": "^6.11.2",
        "undici": "^6.6.0"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.6",
        "@types/jest": "^29.5.12",
        "@types/node": "^20.11.16",
        "@types/pg": "^8.11.0",
        "@types/qs": "^6.9.11",
        "@types/request": "^2.48.12",
        "@typescript-eslint/eslint-plugin": "^6.20.0",
        "@typescript-eslint/parser": "^6.20.0",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "jest": "^29.7.0",
        "nodemon": "^3.0.3",
        "prettier": "^3.2.4",
        "ts-jest": "^29.1.2",
        "ts-node": "^10.9.2",
        "tsc-alias": "^1.8.8",
        "tsc-watch": "^6.0.4",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.3.3"
    }
}
