{
  "include": [
    "src/**/*",
    "src/*",
    "test/**/*.test.ts",
    "test/**/*.ts",
    "test/*.ts"
, "test/utils/ressourceCreator/createMovie.ts", "test/routes/auth.routes.test.old", "test/routes/auth.routes.test.old", "test/routes/metrics.routes.test.old", "test/routes/metrics.routes.test.old", "test/routes/movies.routes.test.old", "test/routes/movies.routes.test.old", "test/routes/users.routes.test.old", "test/routes/users.routes.test.old", "test/routes/propositions.routes.test.old", "test/routes/propositions.routes.test.old", "test/routes/reviews.routes.test.ts_old", "test/routes/reviews.routes.test.ts_old"  ],
  "exclude": ["dist"],
  "compilerOptions": {
    "types": ["@types/jest", "node"],
    "moduleResolution": "node",
    "target": "es2022",
    "module": "commonjs",
    "lib": ["es2022", "esnext.asynciterable"],
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "removeComments": true,
    "sourceMap": true,
    "baseUrl": ".",
    "outDir": "dist",
    "paths": {
      "@custom_types/*":["src/types/*"],
      "@hooks/*": ["src/hooks/*"],
      "@modules/*": ["src/modules/*"],
      "@plugins/*": ["src/plugins/*"],
      "@routes/*": ["src/routes/*"],
      "@schemas/*": ["src/schemas/*"],
      "@src/*": ["src/*"],
      "@utils/*": ["src/utils/*"]
    }
  }
}
